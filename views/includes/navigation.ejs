      <nav class="navbar navbar-expand-lg navbar-dark bg-success" >


              <div class="navbar-header">
                  <a class="navbar-brand" href="/">Book Store</a>
                </div>

              <button class="navbar-toggler" class="<%= path === '/' ? 'active' : '' %>" type="button" data-toggle="collapse" data-target="#navbarNav" aria-controls="navbarNav" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
              </button>
              <div class="collapse navbar-collapse" id="navbarNav" >
                  <ul class="navbar-nav">
                    <li class="nav-item">
                      <a class="nav-link" class="<%= path === '/' ? 'active' : '' %>" href="/">Home <span class="sr-only">(current) </a>
                    </li>

                    <% if (isAuthenticated&&Isadmin) { %>
                      <li class="nav-item">
                          <a class="nav-link" class="<%= path === '/cart' ? 'active' : '' %>" href="/cart">Cart</a>
                      </li>
                      <li class="nav-item">
                          <a class="nav-link" class="<%= path === '/orders' ? 'active' : '' %>" href="/orders">Orders</a>
                      </li>
                      <li class="nav-item">
                          <a class="nav-link" class="<%= path === '/admin/allOrders' ? 'active' : '' %>" href="admin/allorders">AllOrders</a>
                      </li>
                      <li class="nav-item">
                          <a class="nav-link" class="<%= path === '/admin/add-product' ? 'active' : '' %>" href="/admin/add-product">Add Book
                          </a>
                      </li>
                      <li class="nav-item">
                          <a class="nav-link" class="<%= path === '/admin/products' ? 'active' : '' %>" href="/admin/products">Admin Books
                          </a>
                      </li>
                  <% } %>
                  <% if (isAuthenticated&&!Isadmin) { %>
                      <li class="nav-item active">
                          <a class="nav-link" class="<%= path === '/cart' ? 'active' : '' %>" href="/cart">Cart</a>
                      </li>
                      <li class="nav-item">
                          <a class="nav-link" class="<%= path === '/orders' ? 'active' : '' %>" href="/orders">Orders</a>
                      </li>

                  <% } %>
              </ul>

                  <% if (!isAuthenticated) { %>
                <ul  class="navbar-nav navbar-right" >
                      <li class="nav-item" >
                          <a class="nav-link" class="<%= path === '/login' ? 'active' : '' %>" href="/login"><span class="glyphicon glyphicon-log-in"></span> Login</a>
                      </li>
                      <li class="nav-item">
                          <a class="nav-link" class="<%= path === '/signup' ? 'active' : '' %>" href="/signup"><span class="glyphicon glyphicon-user"></span>Signup</a>
                      </li>
                        </ul>

                  <% } else { %>
                        <ul  class="navbar-nav navbar-right" >

                      <li class="nav-item">
                          <form action="/logout" method="post">
                              <input type="hidden" name="_csrf" value="<%= csrfToken %>">
                              <button class="nav-link"  type="submit">Logout</button>
                          </form>
                      </li>
                        </ul>
                  <% } %>


              </div>

          </nav>









